envId: {{ hostvars['localhost'].apocng_uuid }}
providers:
  minikube:
    enabled: false
  aws:
    enabled: true

nginxBasicAuth:
  service:
    type: LoadBalancer

spinnaker:
  rbac:
    enabled: true
  redis:
    persistence:
      enabled: true
      storageClass: gp2
  minio:
    replicas: 4
    persistence:
      type: PersistentVolumeClaim
      storageClass: gp2

  ui:
    service:
      type: ClusterIP

loghouse:
  rbac:
    enabled: true
  service:
    type: LoadBalancer
